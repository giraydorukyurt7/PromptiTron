# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Security
SECRET_KEY=your_secret_key_here_change_in_production

# Application Settings
DEBUG=true
APP_NAME=Promptitron AI
APP_VERSION=2.0.0

# Database (optional)
DATABASE_URL=sqlite:///./promptitron.db
REDIS_URL=redis://localhost:6379

# File Storage
UPLOAD_DIR=uploads
MAX_UPLOAD_SIZE=104857600

# ChromaDB Settings
CHROMA_PERSIST_DIR=chroma_db
CHROMA_COLLECTION_NAME=yks_documents

# Content Settings
JSON_DIR=data/jsonn
CACHE_TTL=3600

# Model Configuration
TEMPERATURE=0.7
MAX_OUTPUT_TOKENS=4096
TOP_P=0.95
TOP_K=40

# Function Calling Configuration
FUNCTION_CALLING_MODE=AUTO
MAX_FUNCTION_CALLS=10

# Embedding Configuration
EMBEDDING_DIMENSIONS=768
EMBEDDING_TASK_TYPE=SEMANTIC_SIMILARITY

# Safety Settings
SAFETY_FILTER_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
SAFETY_FILTER_HATE_SPEECH=BLOCK_MEDIUM_AND_ABOVE
SAFETY_FILTER_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
SAFETY_FILTER_DANGEROUS_CONTENT=BLOCK_MEDIUM_AND_ABOVE

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60